<launch>
    <node pkg="rosserial_python" type="serial_node.py" name="isr_m2_gw_node">
        <param name="port" value="/dev/ttyACM0" />
        <param name="baud" value="57600" />
    </node>
    <node pkg="isr_m2_driver" type="isr_m2_driver_node" name="isr_m2_driver_node">
    </node>
    <!--<node pkg="isr_m2_controllers" type="isr_m2_manual_controller_node" name="isr_m2_manual_controller_node">
    </node> -->
    <node pkg="isr_m2_controllers" type="manual_controller_node.py" name="isr_m2_manual_controller_node">
    </node>
    <node pkg="keyboard" type="keyboard" name="keyboard_node">
        <param name="allow_repeat" value="false" />
        <param name="repeat_delay" value="SDL_DEFAULT_REPEAT_DELAY" />
        <param name="repeat_interval" value="SDL_DEFAULT_REPEAT_INTERVAL" />
    </node>
    <node pkg="lms1xx" type="LMS1xx_node" name="lms1xx_node">
        <param name="host" value="192.168.1.211" />
        <param name="frame_id" value="lrf" />
    </node>
</launch>
